#set( $ContextPath = ${request.contextPath} )
#set($layout="layout/layoutACE.vm")

<link rel="stylesheet" href="http://tablesorter.com/themes/blue/style.css" type="text/css" id="" media="print, projection, screen" />

<script type="text/javascript" src="${ContextPath}/assets/js/jquery-1.10.2.min.js"></script>
<script type="text/javascript" src="http://tablesorter.com/__jquery.tablesorter.min.js"></script>
<script type="text/javascript" src="${ContextPath}/js/ajaxfileupload.js"></script>
<script>
	$(function(){
		
		$("#sample-table-1").tablesorter();
	});

	function refreshRank(){
		
		$.ajax({
			url  : "exam/refresh",
			type : 'POST',
			cache: false,
			contentType: 'application/json; charset=utf-8',
			success: function(json) { alert("success"); },
		    error: function(msg) { alert("error"); }
		});
	}
</script>

<div class="row">
	<div class="space-6">
	</div>
	<div class="col-sm-6">
		<table class="tablesorter table table-striped .table-bordered table-hover" cellspacing="1" id='sample-table-1'>             
		    <thead>
		        <tr>
		        	<th>userName</th> 
		            <th>total</th> 
		            <th>rate</th> 
		            <th>checkIns</th>
		        </tr> 
		    </thead> 
		    <tbody>
		    	#foreach(${client} in ${clients})
		        <tr> 
		            <td>${client.userName}</td> 
		            <td>${client.total}</td> 
		            <td>${client.correctRate}%</td> 
		            <td>${client.checkIns}</td> 
		        </tr>
		        #end
		    </tbody> 
		</table>
	</div>
</div>