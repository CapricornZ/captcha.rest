<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Hibernate-mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="demo.captcha.model.Resource" table="resource">
    	<cache usage="read-write"/>
        <id name="id" type="java.lang.Integer">
            <column name="id"/>
            <generator class="identity"/>
        </id>
        <property name="tag"/>
        <property name="env"/>
        <property name="content" column="jsonContent" length="2048"/>
        <property name="updateTime" type="java.sql.Timestamp" column="updateTime"/>
    </class>
</hibernate-mapping>